<?xml version="1.0"?>
<configuration>
	<configSections>
		<section name="fps" type="TextAdventure.WindowsGame.Configuration.FpsConfigurationSection, TextAdventure.WindowsGame"/>
		<section name="log" type="TextAdventure.WindowsGame.Configuration.LogConfigurationSection, TextAdventure.WindowsGame"/>
		<section name="worldTime" type="TextAdventure.WindowsGame.Configuration.WorldTimeConfigurationSection, TextAdventure.WindowsGame"/>
		<section name="volume" type="TextAdventure.WindowsGame.Configuration.VolumeConfigurationSection, TextAdventure.WindowsGame"/>
	</configSections>
	<fps visible="true"/>
	<log visible="false"
		 maximumVisibleLogLines="30"
		 minimumWindowWidth="150"
		 logEntryLifetime="1.00:00:00"
		 showTimestamps="true"/>
	<worldTime visible="true"/>
	<volume soundEffects="1.0" music="1.0"/>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
	</startup>
</configuration>
